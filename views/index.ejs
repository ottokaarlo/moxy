<body id="body" ng-app="ecosquared" ng-cloak>
   <div ng-controller="mainCtrl as m">
      <!-- Modals Begin -->
      <% include modules/help-menu2 %>
      <!-- Modals End -->

      <!-- Navbar Begin -->
      <% include modules/navbar %>
      <!-- Navbar End -->

      <!-- Container Begin -->
      <div class="container innerAll animated fadeIn ThreeSecDelay">
         <div class="panel panel-default">
            <div class="panel-heading" role="tab" style="border-bottom: 0">
               <h4 class="panel-title text-center">
                  <button type="button" class="btn btn-default pull-left" style="margin-top: -7px; padding-top: 7px" data-toggle="modal" data-target="#modal-user-info">
                     <i class="fa fa-fw icon-visual-eye"></i>
                  </button>
                  <span style="margin-left: -30px">Balance: <span ng-show="m.load">Loading...</span> <span ng-show="m.pound">Â£</span> {{ m.Balance | number:2 }}</span>
               </h4>
            </div>
         </div>
         <!-- Panels Begin -->
         <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true" ng-hide="m.panels">
            <!-- Product Begin -->
            <% include modules/prod %>
            <!-- Product End -->

            <!-- Project Begin -->
            <% include modules/project %>
            <!-- Project End -->

            <!-- Person Begin -->
            <% include modules/person %>
            <!-- Person End -->
         </div>
         <% include modules/create %>
         <% include modules/invite %>
         <% include modules/accept %>
         <!-- Panels End -->
      </div>
      <!-- Container End -->

      <!-- Footer Begin -->
      <% include modules/footer %>
      <!-- Footer End -->
   </div>





























   <!-- POST FORMS  -->
   <script src="/../js/forms.js"></script>
